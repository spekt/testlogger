<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!--To inherit the global NuGet package sources remove the <clear/> line below -->
    <clear />
    <add key="nuget" value="https://api.nuget.org/v3/index.json" />
    <add key="nunit-pre" value="https://www.myget.org/F/nunit/api/v3/index.json " />
  </packageSources>

  <!-- No package source mapping for test assets to allow traditional fallback behavior -->
  <!-- This allows both stable versions from nuget.org and alpha versions from MyGet -->
</configuration>